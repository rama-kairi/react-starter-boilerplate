    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/types": "^17.0.0",
    "@jest/types": "^28.1.3",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.17.1",
