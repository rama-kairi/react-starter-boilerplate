    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
